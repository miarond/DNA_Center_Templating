        "softwareType": "IOS",
        "softwareVariant": "XE",
        "templateContent": "{# ACCESS PORT CONFIGURATION MACRO #}\n{% macro access_port(description, access_vlan, voice_vlan) %}\n{% if description != \"\" %} \n description {{ description }}\n{% endif %}\n switchport mode access\n{% if access_vlan != \"\" %}\n switchport access vlan {{ access_vlan }}\n{% else %}\n switchport access vlan 999\n{% endif %}\n{% if voice_vlan != \"\" %}\n switchport voice vlan {{ voice_vlan }}\n{% endif %}\n switchport port-security\n device-tracking attach-policy IPDT_MAX_10\n spanning-tree portfast\n no shutdown\n{% endmacro %}\n\n{# TRUNK PORT CONFIGURATION MACRO #}\n{% macro trunk_port(description, allowed_vlans) %}\n{% if description != \"\" %} \n description {{ description }}\n{% endif %}\n switchport mode trunk\n{% if allowed_vlans != \"\" %}\n switchport trunk allowed vlan {{ allowed_vlans }}\n{% else %}\n switchport trunk allowed vlan none\n{% endif %}\n no shutdown\n{% endmacro %}\n\n\n{# UPDATE TRUNK PORT WITH PORTCHANNEL CONFIG #}\n{% macro trunk_portchannel(channel_protocol, channel_group, load_interval, duplex) %}\n{% if channel_protocol != \"\" %}\n channel-protocol {{ channel_protocol }}\n{% endif %}\n{% if channel_group != \"\" %}\n channel-group {{ channel_group }} mode active\n{% endif %}\n{% if load_interval != \"\" %}\n load-interval {{ load_interval }}\n{% endif %}\n{% if duplex != \"\" %}\n duplex {{ duplex }}\n{% endif %}\n{% endmacro %}\n\n{# PORTCHANNEL INTERFACE CONFIGURATION MACRO #}\n{% macro portchannel_interface(description, switchport_mode, allowed_vlans, access_vlan) %}\n{% if description != \"\" %} \n description {{ description }}\n{% endif %}\n{% if allowed_vlans != \"\" %}\n switchport mode trunk\n switchport trunk allowed vlan {{ allowed_vlans }}\n{% else %}\n switchport mode access\n {% if access_vlan != \"\" %}\n switchport access vlan {{ access_vlan }}\n {% else %}\n switchport access vlan 999\n {% endif %}\n{% endif %}\n no shutdown\n{% endmacro %}",
